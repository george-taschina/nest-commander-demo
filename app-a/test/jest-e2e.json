{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@core/core": "<rootDir>/../libs/core/src",
    "@core/core/(.*)": "<rootDir>/../libs/core/src/$1",
    "@document/document": "<rootDir>/../libs/document/src",
    "@document/document/(.*)": "<rootDir>/../libs/document/src/$1",
    "@user/user": "<rootDir>/../libs/user/src",
    "@user/user/(.*)": "<rootDir>/../libs/user/src/$1"
  }
}